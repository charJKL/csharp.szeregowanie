<UserControl x:Class="Szeregowanie.View.Chart.Scale"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Szeregowanie.View.Chart"
             mc:Ignorable="d" 
             d:DesignHeight="20" d:DesignWidth="20">
    <Grid>
        <TextBlock x:Name="Value" Foreground="#000000" HorizontalAlignment="Center" VerticalAlignment="Center" />
        <Line X1="0" X2="0" Y2="4" Stroke="Black" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,-2,0,0" />    
    </Grid>
</UserControl>

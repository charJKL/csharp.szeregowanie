<UserControl x:Class="Szeregowanie.View.Chart.Label"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Szeregowanie.View.Chart"
             mc:Ignorable="d" 
             d:DesignHeight="20" d:DesignWidth="100">
    <Grid>
        <TextBlock x:Name="LabelName" Foreground="#000000" HorizontalAlignment="Right" VerticalAlignment="Center" Padding="0,0,5,0" />
        <Line HorizontalAlignment="Right" VerticalAlignment="Top" Width="4" Height="2" Stroke="Black" StrokeThickness="1" X2="4" Margin="0,0,-2,0"  />
    </Grid>
</UserControl>
